{
	"name": "friendly-frontend-starter",
	"version": "1.0.0",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/AleksanderLamkov/friendly-frontend-starter.git"
	},
	"type": "module",
	"scripts": {
		"start": "minista --open --host",
		"dev": "minista --open --host",
		"build": "minista build",
		"preview": "minista preview --open --host",
		"lint:js": "eslint src --ext .js,.jsx",
		"lint:js:fix": "eslint src --ext .js,.jsx --fix",
		"lint:css": "stylelint \"**/*.{css,scss,pcss}\"",
		"lint:css:fix": "stylelint \"**/*.{css,scss,pcss}\" --fix",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"lint": "run-s lint:js lint:css format",
		"lint:fix": "run-s lint:js:fix lint:css:fix format:fix",
		"fonts:convert": "node tasks/fonts-converter.js",
		"fonts:css": "node tasks/fonts-style.js",
		"fonts": "run-s fonts:convert fonts:css",
		"cssimg": "node tasks/css-image-set.js",
		"htmlcssimg": "node tasks/create-img-for-css.js",
		"build:cssimg": "run-s htmlcssimg && minista build && cssimg",
		"mk": "node tasks/create-component.js"
	},
	"devDependencies": {
		"@babel/eslint-parser": "7.28.0",
		"@babel/plugin-syntax-jsx": "7.27.1",
		"@eslint/js": "9.30.1",
		"eslint": "9.30.1",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-prettier": "5.5.1",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "5.2.0",
		"fontmin": "^2.0.0",
		"friendly-frontend-lint-config": "1.0.2",
		"globals": "16.3.0",
		"minista": "3.1.12",
		"normalize.css": "^8.0.1",
		"npm-run-all": "4.1.5",
		"postcss-preset-env": "10.2.4",
		"postcss-pxtorem": "6.1.0",
		"prettier": "3.6.2",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"sass": "1.89.2",
		"stylelint": "16.21.0",
		"stylelint-config-standard-scss": "15.0.1",
		"stylelint-order": "7.0.0",
		"stylelint-scss": "6.12.1",
		"stylelint-selector-bem-pattern": "4.0.1"
	},
	"dependencies": {
		"@a1rth/css-normalize": "1.2.1",
		"clsx": "2.1.1",
		"imask": "7.6.1",
		"otf2ttf": "^1.1.2",
		"swiper": "^11.2.10",
		"ttf2eot": "^3.1.0",
		"ttf2woff": "^3.0.0",
		"ttf2woff2": "^8.0.0"
	},
	"overrides": {
		"typescript-eslint": {
			"eslint": "9.30.1"
		},
		"@typescript-eslint/parser": {
			"eslint": "9.30.1"
		},
		"@typescript-eslint/eslint-plugin": {
			"eslint": "9.30.1"
		}
	},
	"browserslist": [
		">0.5%",
		"last 2 versions",
		"not dead"
	]
}
